<!DOCTYPE html>
<html>
  <head>
    <title>Farolito</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': true %>

    <script src="//maps.google.com/maps/api/js?key=<%= DenunciaController.new.getKey %>"></script>
	   <script src="//cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js"></script>
  </head>

  <body>
    <!-- Header 
      ================================================== -->
      <header id="header-container">
          <!-- Header -->
          <div id="header">
              <div class="container">                
                  <!-- Contenido Izquierda -->
                  <div class="left-side">                    
                      <!-- Navegación Móvil  -->
                      <div class="mmenu-trigger">
                          <button class="hamburger hamburger--collapse" type="button">
                              <span class="hamburger-box">
                                  <span class="hamburger-inner"></span>
                              </span>
                          </button>
                      </div>
                      <!-- Navegacion Principal - Menu  -->
                      <nav id="navigation" class="style-1">
                          <ul id="responsive">
                              <li>      
                                <a href="#">Menu</a>     
                                <div class="mega-menu mobile-styles one-columns">
                                  <div class="mega-menu-section">
                                    <ul>
                                      <li class="mega-menu-headline">Mantenimientos
                                        <li>
                                          <a href="/catalogos">Catalogos</a>
                                          <a href="/institucions">Instituciones</a>
                                          <a href="/denuncia">Denuncias</a>
                                        </li>
                                      </li>
                                    </ul>
                                  </div>
                                </div>                              
                              </li>
                          </ul>
                      </nav>                    
                  </div>
                  <!-- Contenido Derecha / Fin -->
              </div>
          </div>          
          <!-- Header / fin -->
      </header>
    <%= yield %>
  </body>
</html>
